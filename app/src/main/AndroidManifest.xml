<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.saludtec.testimages"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".RotateResizeMoveActivity"
            android:label="@string/app_name"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName=".Main2Activity">
        </activity>
        <activity
            android:name=".MaskActivity"
            android:label="@string/title_activity_mask"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".CloneActivity"
            android:label="@string/title_activity_clone"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".FotoguiasActivity"
            android:label="@string/title_activity_fotoguias"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".ZoomActivity"
            android:label="@string/title_activity_zoom"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".BitmapMeshAcivity"
            android:label="@string/title_activity_bitmap_mesh_acivity"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".Main2Activity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>
